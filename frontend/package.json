    "@mui/material": "^7.0.1",
    "@openzeppelin/contracts": "^5.0.1",
    "@react-spring/three": "^9.7.3",
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.1.0",
