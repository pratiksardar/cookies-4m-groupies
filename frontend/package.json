{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "compile": "hardhat compile",
    "deploy:alfajores": "node --loader ts-node/esm scripts/deploy.ts --network alfajores",
